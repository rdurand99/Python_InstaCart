      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "#Created lineplot\n",
    "hourly_spending_line = plt.plot(hourly_spending.index, hourly_spending, marker='o', linestyle='-')\n",
    "plt.xlabel('Hour of the Day')\n",
    "plt.ylabel('Total Spending')\n",
    "plt.title('Spending Patterns by Hour')\n",
    "plt.savefig(os.path.join(path, '04 Analysis', 'Visualizations', 'hourly_spending_line.png'))\n",
    "plt.show()"
   ]
