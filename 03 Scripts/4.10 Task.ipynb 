   ]
  },
  {
   "cell_type": "code",
   "execution_count": 56,
   "id": "47a31de1",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Created department profile (Vegan, Vegetarian, Meat-consumer)\n",
    "diet_dict = dict()\n",
    "for user in dept_user_cross:\n",
    "    workinglist = dept_user_cross[user].tolist()\n",
    "    if workinglist[11] == 0 and workinglist[15] == 0: \n",
    "        diet_dict[user] = 'Vegan'\n",
    "    elif workinglist[11] == 0:\n",
    "        diet_dict[user] = 'Vegetarian'\n",
    "    else:\n",
    "        diet_dict[user] = 'Meat-consumer'"
   ]
  },
